_go_completer()
{
    local current=$2
    local previous=$3
    echo "'$previous' '$current' '${COMP_WORDS[@]}'" >> f

    if [ $COMP_CWORD -eq 1 ]
    then
        local wordlist="bug build clean doc env fix fmt generate get install list mod work run test tool version vet"
        COMPREPLY=($(compgen -W "$wordlist" $current))
        return
    fi

    if [ ${COMP_WORDS[1]} = bug ]
    then
        COMPREPLY=()
        return
    fi


    compopt -o default
}

complete -F _go_completer go
