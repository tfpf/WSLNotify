{
    "debug.console.fontSize": 16,
    "editor.autoClosingBrackets": "never",
    "editor.autoClosingComments": "never",
    "editor.autoClosingDelete": "never",
    "editor.autoClosingOvertype": "never",
    "editor.autoClosingQuotes": "never",
    "editor.autoIndent": "keep",
    "editor.autoSurround": "never",
    "editor.cursorBlinking": "solid",
    "editor.cursorSurroundingLines": 3,
    "editor.fontFamily": "'CaskaydiaCove Nerd Font', 'Droid Sans Mono', 'monospace', monospace",
    "editor.fontLigatures": true,
    "editor.fontSize": 16,
    "editor.minimap.maxColumn": 79,
    "editor.minimap.renderCharacters": false,
    "editor.occurrencesHighlight": "off",
    "editor.renderFinalNewline": "dimmed",
    "editor.renderWhitespace": "trailing",
    "editor.rulers": [
        79,
        119
    ],
    "editor.tokenColorCustomizations": {
        "textMateRules": [
            {
                "settings": {
                    "fontStyle": "italic",
                    "foreground": "#667777"
                },
                "scope": "comment"
            },
        ]
    },
    "files.eol": "\n",
    "files.insertFinalNewline": true,
    "files.trimTrailingWhitespace": true,
    "telemetry.telemetryLevel": "off",
    "terminal.integrated.cursorStyle": "line",
    "terminal.integrated.fontSize": 16,
    "vim.highlightedyank.color": "#27B4A63D",
    "vim.highlightedyank.duration": 300,
    "vim.highlightedyank.enable": true,
    "vim.hlsearch": true,
    "vim.ignorecase": false,
    "vim.report": 1,
    "workbench.colorCustomizations": {
        "editor.findMatchBackground": "#B4A6278D",
        "editor.findMatchHighlightBackground": "#27B4A68D",
        "editor.selectionBackground": "#27B4A63D",
        "editor.selectionHighlightBackground": "#B4A6273D"
    }
}
